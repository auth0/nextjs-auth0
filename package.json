{
  "name": "@auth0/nextjs-auth0",
  "version": "0.16.0",
  "description": "Next.js SDK for signing in with Auth0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.browser.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": "^10.13.0 || >=12.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "pretty": "prettier --write \"src/**/*.ts\" \"src/*.ts\"",
    "lint": "eslint --fix --ext .ts ./src",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest tests/hooks/ --coverage --maxWorkers=10",
    "test:watch": "jest --coverage --watch",
    "prepublishOnly": "npm test && npm run lint",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0/nextjs-auth0.git"
  },
  "keywords": [
    "auth0",
    "next.js",
    "react",
    "oidc",
    "authentication",
    "vercel"
  ],
  "author": "Auth0 (https://auth0.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/nextjs-auth0/issues"
  },
  "homepage": "https://github.com/auth0/nextjs-auth0#readme",
  "devDependencies": {
    "@panva/jose": "^1.9.3",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/clone": "^2.1.0",
    "@types/cookie": "^0.3.3",
    "@types/hapi__iron": "^5.1.0",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/node": "^13.13.5",
    "@types/on-headers": "^1.0.0",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/request": "^2.48.4",
    "@types/webpack": "^4.41.24",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^26.6.2",
    "next": "^9.4.0",
    "nock": "^12.0.3",
    "prettier": "^2.0.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "request": "^2.88.2",
    "timekeeper": "^2.2.0",
    "ts-jest": "^26.4.3",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/debug": "^4.1.5",
    "@types/http-errors": "^1.8.0",
    "@types/url-join": "^4.0.0",
    "base64url": "^3.0.1",
    "clone": "^2.1.2",
    "cookie": "^0.4.1",
    "debug": "^4.2.0",
    "futoin-hkdf": "^1.3.2",
    "http-errors": "^1.8.0",
    "jose": "^2.0.2",
    "on-headers": "^1.0.2",
    "openid-client": "^4.0.0",
    "url-join": "^4.0.1"
  },
  "peerDependencies": {
    "next": "^9.4.0"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/",
      "./jest.config.js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "preset": "ts-jest"
  }
}
